<apex:page standardController="Account">
    <apex:form id="fm">
        <apex:inputHidden value="{!Account.Name}" id="fb"/>
        <apex:commandButton value="Click" onclick="show()"/>
    <script>
        function show()
        {
         var myname=document.getElementById('{!$Component.fb}').value;
         alert(myname);
         ]
        </script>
    </apex:form>
</apex:page>